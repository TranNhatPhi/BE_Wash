{
    "version": 2,
    "builds": [
        {
            "src": "api/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/index.js"
        }
    ],
    "env": {
        "DB_HOST": "@db_host",
        "DB_PORT": "@db_port",
        "DB_NAME": "@db_name",
        "DB_USER": "@db_user",
        "DB_PASSWORD": "@db_password",
        "DB_SSL": "@db_ssl",
        "JWT_SECRET": "@jwt_secret",
        "JWT_EXPIRES_IN": "@jwt_expires_in"
    }
}